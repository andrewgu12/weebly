doctype html
html
	head
		title= title
		link(rel="stylesheet",href='/stylesheets/apiStyles.css')
	body
		div(id="wrapper")
			div(id="weebly_logo")
				img(src="/images/weebly_logo.png")
				div(id="login")
					form(id='apiForm',method='post')
						div
							label API Token: 
							input(type='text',id='username')
							br
							label Email: 
							input(type='text',id='password')
						input(type='submit', value='submit')